{
  "name": "vite-plugin-markdoc",
  "version": "0.0.4",
  "packageManager": "pnpm@7.0.0",
  "description": "Markdoc for Vite",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.1.1",
    "@rollup/pluginutils": "^4.2.1",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@antfu/ni": "^0.16.2",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "pnpm": "^7.1.0",
    "typescript": "4.6.4",
    "unbuild": "^0.7.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
