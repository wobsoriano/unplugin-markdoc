{
  "name": "unplugin-markdoc",
  "version": "0.3.0",
  "packageManager": "pnpm@8.5.0",
  "description": "Markdoc plugin for Vite and Webpack",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/unplugin-markdoc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/unplugin-markdoc.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "frontmatter",
    "markdown"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "<=4.9": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"tsup --watch\" \"vite serve dev\"",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "build:fix": "tsx scripts/postbuild.mts",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@markdoc/markdoc": "^0.2.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@markdoc/markdoc": "^0.2.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "bumpp": "^9.1.0",
    "chalk": "^5.2.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "fast-glob": "^3.2.12",
    "rollup": "^3.21.6",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue": "^3.2.47",
    "webpack": "^5.82.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
