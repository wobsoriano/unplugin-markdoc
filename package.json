{
  "name": "vite-plugin-markdoc",
  "version": "0.0.3",
  "description": "Markdoc for Vite",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast -- --dts-resolve",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm run publish"
  },
  "dependencies": {
    "@markdoc/markdoc": "^0.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@rollup/pluginutils": "^4.2.1",
    "bumpp": "^7.1.1",
    "eslint": "^8.15.0",
    "tsup": "5.11.13",
    "typescript": "4.5.4",
    "vite": "^2.9.9",
    "vitest": "0.2.5"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
